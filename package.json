{
  "name": "yatirimim-trader",
  "version": "0.0.1",
  "author": "KPay",
  "homepage": "https://trader.yatirimim.com/",
  "description": "Yatirimim.com trade application",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "sync": "ionic capacitor sync && node --max-old-space-size=4096 ./api/version.js",
    "prod": "ionic build --prod && npx cap sync && npx cap update && node --max-old-space-size=4096 ./api/version.js",
    "resources": "cordova-res ios && cordova-res android && node --max-old-space-size=4096 ./api/resources.js",
    "ios-run-l": "ionic capacitor run ios -l --external",
    "android-run-l": "ionic capacitor run android -l --external",
    "android-open": "ionic capacitor update android && npx cap open android",
    "ios-open": "ionic capacitor update ios && npx cap open ios",
    "jetify": "npx jetify && npx cap sync android",
    "sloc": "sloc src/",
    "nswag-gen-kpay-backend": "nswag openapi2tsclient /input:https://api.kpay.com.tr/appbackend/KPay.API.AppBackend.V1.yaml /output:src/app/services/api-kpay-backend.service.ts /Template:Angular /InjectionTokenType:InjectionToken /OperationGenerationMode:MultipleClientsFromFirstTagAndOperationId /ClassName:KpayBackend{controller}ApiService /DateTimeType:MomentJS /BaseUrlTokenName:KPAY_BACKEND_API_URL && node --max-old-space-size=4096 ./api/api.js",
    "nswag-gen-kpay-ccpayment": "nswag openapi2tsclient /input:https://api.kpay.com.tr/ccpayment/KPay.API.CCPayment.V1.yaml /output:src/app/services/api-kpay-ccpayment.service.ts /Template:Angular /InjectionTokenType:InjectionToken /OperationGenerationMode:MultipleClientsFromFirstTagAndOperationId /ClassName:KpayCcpayment{controller}ApiService /DateTimeType:MomentJS /BaseUrlTokenName:KPAY_CCPAYMENT_API_URL && node --max-old-space-size=4096 ./api/api.js",
    "nswag-gen-kpay-fixedqr": "nswag openapi2tsclient /input:https://api.kpay.com.tr/fixedqr/KPay.API.FixedQR.V1.yaml /output:src/app/services/api-kpay-fixedqr.service.ts /Template:Angular /InjectionTokenType:InjectionToken /OperationGenerationMode:MultipleClientsFromFirstTagAndOperationId /ClassName:KpayFixedqr{controller}ApiService /DateTimeType:MomentJS /BaseUrlTokenName:KPAY_FIXEDQR_API_URL && node --max-old-space-size=4096 ./api/api.js",
    "nswag-gen-kpay-fixedqrpayment": "nswag openapi2tsclient /input:https://api.kpay.com.tr/fixedqrpayment/KPay.API.FixedQRPayment.V1.yaml /output:src/app/services/api-kpay-fixedqrpayment.service.ts /Template:Angular /InjectionTokenType:InjectionToken /OperationGenerationMode:MultipleClientsFromFirstTagAndOperationId /ClassName:KpayFixedqrPayment{controller}ApiService /DateTimeType:MomentJS /BaseUrlTokenName:KPAY_FIXEDQRPAYMENT_API_URL && node --max-old-space-size=4096 ./api/api.js",
    "nswag-gen-kpay": "npm run nswag-gen-kpay-backend && npm run nswag-gen-kpay-ccpayment && npm run nswag-gen-kpay-fixedqr && npm run nswag-gen-kpay-fixedqrpayment",
    "nswag-gen-yatirimim": "nswag openapi2tsclient /input:http://api.yatirimim.com/apidoc/v1/api.json?key=yatirimim /output:src/app/services/api-yatirimim.service.ts /Template:Angular /InjectionTokenType:InjectionToken /OperationGenerationMode:MultipleClientsFromPathSegments /ClassName:{controller}ApiService /DateTimeType:MomentJS /BaseUrlTokenName:MARITZA_API_URL && node --max-old-space-size=4096 ./api/api.js",
    "nswag-gen-hkn-yatirimim": "nswag openapi2tsclient /input:https://api.yatirimim.com/AppBackend/Maritza.API.AppBackend.V1.yaml /output:src/app/services/api-hkn-yatirimim.service.ts /Template:Angular /InjectionTokenType:InjectionToken /OperationGenerationMode:MultipleClientsFromFirstTagAndOperationId /ClassName:{controller}ApiService /DateTimeType:MomentJS /BaseUrlTokenName:MARITZA_API_URL && node --max-old-space-size=4096 ./api/api.js",
    "nswag-gen-yatirimim-local": "nswag openapi2tsclient /input:http://192.168.253.92:5000/swagger/v1/swagger.json /output:src/app/services/api-yatirimim.service.ts /Template:Angular /InjectionTokenType:InjectionToken /OperationGenerationMode:MultipleClientsFromPathSegments /ClassName:{controller}ApiService /DateTimeType:MomentJS /BaseUrlTokenName:MARITZA_API_URL && node --max-old-space-size=4096 ./api/api.js"
  },
  "dependencies": {
    "@angular/common": "^14.0.0",
    "@angular/core": "^14.0.0",
    "@angular/forms": "^14.0.0",
    "@angular/platform-browser": "^14.0.0",
    "@angular/platform-browser-dynamic": "^14.0.0",
    "@angular/router": "^14.0.0",
    "@awesome-cordova-plugins/barcode-scanner": "^5.44.0",
    "@awesome-cordova-plugins/core": "^5.44.0",
    "@capacitor/android": "3.6.0",
    "@capacitor/app": "^1.1.1",
    "@capacitor/clipboard": "^1.0.8",
    "@capacitor/core": "^3.6.0",
    "@capacitor/haptics": "^1.1.4",
    "@capacitor/ios": "3.6.0",
    "@capacitor/keyboard": "^1.2.3",
    "@capacitor/status-bar": "^1.0.8",
    "@ionic/angular": "^6.1.9",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "apexcharts": "^3.35.3",
    "jwt-decode": "^3.1.2",
    "lightweight-charts": "^3.8.0",
    "moment": "^2.29.4",
    "ng-apexcharts": "^1.7.1",
    "phonegap-plugin-barcodescanner": "^8.1.0",
    "primeicons": "^5.0.0",
    "primeng": "^14.0.0-rc.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.0",
    "@angular-eslint/builder": "~13.0.1",
    "@angular-eslint/eslint-plugin": "~13.0.1",
    "@angular-eslint/eslint-plugin-template": "~13.0.1",
    "@angular-eslint/template-parser": "~13.0.1",
    "@angular/cli": "^14.0.0",
    "@angular/compiler": "^14.0.0",
    "@angular/compiler-cli": "^14.0.0",
    "@angular/language-service": "^14.0.0",
    "@capacitor/cli": "^3.6.0",
    "@ionic/angular-toolkit": "^6.0.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "protractor": "~7.0.0",
    "sloc": "^0.2.1",
    "ts-node": "~8.3.0",
    "typescript": "~4.7.3"
  }
}
